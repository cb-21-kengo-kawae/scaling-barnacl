create_table "billing_packets", force: :cascade, options: "ENGINE=InnoDB DEFAULT CHARSET=utf8mb4", comment: "パケット利用量に応じた課金" do |t|
  t.string   "hash_id",      null: false,                   comment: "パケット集計の処理を一意に特定するためのハッシュ値"
  t.integer  "xxxxxx_id",    unsigned: true                 comment: "アプリケーションID"
  t.integer  "xxxxxx_id",    unsigned: true                 comment: "処理ID"
  t.datetime "started_at",   null: false                    comment: "実行開始時間"
  t.datetime "finished_at",  null: false                    comment: "実行終了時間"
  t.text     "query",        null: false,                   comment: "実行されたクエリ"
  t.integer  "packet",       unsigned: true                 comment: "消費パケット数"
  t.datetime "created_at",   null: false
  t.datetime "updated_at",   null: false
  t.index ["hash_id"], name: "billing_packets_references_on_hash_id", using: :btree
end

create_table "billing_packet_dailies", force: :cascade, options: "ENGINE=InnoDB DEFAULT CHARSET=utf8mb4", comment: "パケット利用量に応じた課金（日次のサマリ）" do |t|
  t.string   "date",         null: false,                   comment: "日付（YYYY-MM-DD形式）"
  t.integer  "packet",       unsigned: true                 comment: "消費パケット数"
  t.datetime "created_at",   null: false
  t.datetime "updated_at",   null: false
  t.index ["date"], name: "billing_packet_dailies_references_on_date", using: :btree
end
