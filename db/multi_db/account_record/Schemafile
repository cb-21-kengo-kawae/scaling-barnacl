create_table "billing_fixeds", force: :cascade, options: "ENGINE=InnoDB DEFAULT CHARSET=utf8mb4", comment: "固定費 集計結果" do |t|
  t.string   "date",         null: false,                   comment: "日付（YYYY-MM-DD形式）"
  t.text     "policy_ids",   null: false,                   comment: "利用可能なサービスのpolicy_id（カンマ区切り）"
  t.datetime "created_at",   null: false
  t.datetime "updated_at",   null: false
  t.index ["date"], name: "billing_fixeds_references_on_date", using: :btree
end

